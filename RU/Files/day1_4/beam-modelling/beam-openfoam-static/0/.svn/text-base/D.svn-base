FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    object      D;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions          [ 0 1 0 0 0 0 0 ];

internalField       uniform ( 0 0 0 );

boundaryField
{
    zfaces
    {
        type            tractionDisplacement;
        traction        uniform (0 0 0);
        pressure        uniform 0;
        value           uniform (0 0 0);
    }

    yfaces
    {
        type            tractionDisplacement;
        traction        uniform (0 0 0);
        pressure        uniform 0;
        value           uniform (0 0 0);
    }

    fixed
    {
        type            fixedValue;
        value           uniform (0 0 0);
    }

    traction
    {
        type            tractionDisplacement;
        traction        uniform (0 160 0);
        pressure        uniform 0;
        value           uniform (0 0 0);
    }

}

